{
    "presets": [
        ["env", {
            "targets": {
                "node": 8.10
            }
        }],
        "react"
    ],
    "plugins": [
        "transform-object-rest-spread",
        "transform-es2015-spread",
        "transform-object-assign",
        ["transform-runtime", {
            "polyfill": true,
            "regenerator": false
        }]
    ]
}
